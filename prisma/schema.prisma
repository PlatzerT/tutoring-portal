// learn more about it in the docs: https://pris.ly/d/prisma-schema
// env("DATABASE_URL")

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Contact {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String
  message   String
  createdAt DateTime @default(now())
}
